<div class="top-bar">
  <div class="top-bar-title">WEX</div>

  <div class="content-account">
    <div class="top-bar-left">
      <span class="account-name">State of Maine</span>
      <span class="account-number">#123456789134</span>
    </div>

    <div class="top-bar-right">
      Welcome, {{ session.currentUser.firstName }} {{ session.currentUser.lastName }}
      <span class="sep">|</span>
      <a class="link-logout" {{action 'invalidateSession'}}>Log Out</a>
    </div>
  </div>
</div>
